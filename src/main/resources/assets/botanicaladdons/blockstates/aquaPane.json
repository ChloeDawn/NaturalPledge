{
  "multipart": [
    {   "apply": { "model": "botanicaladdons:aquaPanePost" }},
    {   "when": { "north": true },
      "apply": { "model": "botanicaladdons:aquaPaneSide" }
    },
    {   "when": { "east": true },
      "apply": { "model": "botanicaladdons:aquaPaneSide", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "botanicaladdons:aquaPaneSideAlt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "botanicaladdons:aquaPaneSideAlt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "botanicaladdons:aquaPaneNoside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "botanicaladdons:aquaPaneNosideAlt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "botanicaladdons:aquaPaneNosideAlt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "botanicaladdons:aquaPaneNoside", "y": 270 }
    }
  ]
}
